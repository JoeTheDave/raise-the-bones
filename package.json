{
  "name": "raise-the-bones",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "docker-compose up -d && npm run db:migrate && concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:setup": "docker-compose up -d && npm run db:migrate && npm run db:seed",
    "dev:client": "vite --config client/vite.config.ts",
    "dev:server": "tsx watch server/src/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:with-db": "npm run build:client && npm run build:server && npm run db:generate",
    "build:client": "vite build --config client/vite.config.ts",
    "build:server": "tsc -p server/tsconfig.json",
    "start": "node dist/server/index.js",
    "preview": "vite preview --config client/vite.config.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "prisma": "^6.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
